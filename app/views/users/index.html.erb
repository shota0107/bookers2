<% render 'books/list', user: @user %>



<h1>New book</h1>

  <%= form_with model: Book.new do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Opinion</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Creat Book' %>
  <% end %>
  <h1>Users</h1>

<% @users.each do |user| %>
<p>image</p>
<%= image_tag user.get_profile_image(100,100) %>


  <p>name</p>
  <span><%= @books.title %></span>
  <%= link_to "Show", user_path(@user.id) %>

<% end %>